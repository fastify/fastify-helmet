{
  "name": "fastify-helmet",
  "version": "6.0.0",
  "description": "Important security headers for Fastify",
  "main": "index.js",
  "scripts": {
    "lint": "standard",
    "lint:fix": "standard --fix",
    "test": "standard | snazzy && tap 'test/*.test.js' && npm run typescript",
