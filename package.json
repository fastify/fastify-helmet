  "author": "Matteo Collina <hello@matteocollina.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fastify/fastify-helmet/issues"
  },
  "homepage": "https://github.com/fastify/fastify-helmet#readme",
  "devDependencies": {
    "@types/node": "^17.0.8",
    "fastify": "^3.25.3",
    "pre-commit": "^1.2.2",
    "snazzy": "^9.0.0",
    "standard": "^16.0.4",
    "tap": "^15.1.6",
    "tsd": "^0.19.1",
    "typescript": "^4.5.4"
  },
  "dependencies": {
    "fastify-plugin": "^3.0.0",
    "helmet": "^5.0.1"
  }
